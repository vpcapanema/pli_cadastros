# Configuração para forçar HTML
root = true

[*.html]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.htm]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[docs/**/*.html]
charset = utf-8

[views/**/*.html]
charset = utf-8

[templates/**/*.html]
charset = utf-8
